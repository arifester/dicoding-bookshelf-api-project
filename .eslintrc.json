{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "new-cap": "off",
        "no-console": "off",
        "no-unused-vars": "warn",
        "valid-jsdoc": "off",
        "require-jsdoc": "off"
    }
}
